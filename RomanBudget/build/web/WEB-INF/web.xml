<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>LoginFilter</filter-name>
        <filter-class>filter.LoginFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>LoginFilter</filter-name>
        <url-pattern>/region</url-pattern>
        <url-pattern>/consul</url-pattern>
        <url-pattern>/versaille</url-pattern>
        <url-pattern>/financial-transaction</url-pattern>
        <url-pattern>/tax</url-pattern>
        <url-pattern>/king-request</url-pattern>
        <url-pattern>/report</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegionServlet</servlet-name>
        <servlet-class>controller.RegionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsulServlet</servlet-name>
        <servlet-class>controller.ConsulServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VersailleServlet</servlet-name>
        <servlet-class>controller.VersailleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FinancialTransactionServlet</servlet-name>
        <servlet-class>controller.FinancialTransactionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TaxServlet</servlet-name>
        <servlet-class>controller.TaxServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KingRequestServlet</servlet-name>
        <servlet-class>controller.KingRequestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportServlet</servlet-name>
        <servlet-class>controller.ReportServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>controller.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateServlet</servlet-name>
        <servlet-class>controller.UpdateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegionServlet</servlet-name>
        <url-pattern>/region</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsulServlet</servlet-name>
        <url-pattern>/consul</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VersailleServlet</servlet-name>
        <url-pattern>/versaille</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FinancialTransactionServlet</servlet-name>
        <url-pattern>/financial-transaction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TaxServlet</servlet-name>
        <url-pattern>/tax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KingRequestServlet</servlet-name>
        <url-pattern>/king-request</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportServlet</servlet-name>
        <url-pattern>/report</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateServlet</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
</web-app>
